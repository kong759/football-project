<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mappers.TeamPageMapper">
	<update id="updateTeamId" parameterType="HashMap">
		update member set
		team_id = #{teamId} where member.id = #{memberId}
	</update>
</mapper>